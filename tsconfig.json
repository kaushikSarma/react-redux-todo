{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es6",
        "lib": [ "es2015", "es2017", "dom" ],
        "jsx": "react",
        "removeComments": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "baseUrl": "./src",
        "paths": {
            "@components/*": [ "components/*" ],
            "@data/*": [ "data/*" ],
        },
    }
}